#include <iostream>
#include<math.h>
#include<map>
using namespace std;

int factorial(int N){
	if(N<=0)
		return 1;
	return N*factorial(N-1);
}
int magicFraction(int N){
	map<int,int> m;
	if(N==1)
		return 0;
	if(N==2)
		return 1;

	int fact=factorial(N);
	for(int i=2;i<=fact/2;i++){
		if(N%i==0){
			m[i]++;
		}
	}
	int cnt=0;
	map<int,int>::iterator it;
	for(it=m.begin();it!=m.end();it++){
		cnt++;
	}
	return cnt;
}

#include <iostream>
#include<stdlib.h>
using namespace std;

struct linkedNode{
	int data;
	struct linkedNode* next;
};

struct linkedNode* newnode(int data){
	struct linkedNode* new_n=(struct linkedNode*)malloc(sizeof(struct linkedNode));
		new_n->next=NULL;
		new_n->data=data;
		return new_n;
}



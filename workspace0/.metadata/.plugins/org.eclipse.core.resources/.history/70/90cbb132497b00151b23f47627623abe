#include <iostream>
#include<stdlib.h>
using namespace std;
struct node{
	int data;
	struct node* left;
	struct node* right;
};

struct node newnode(int data){
	struct node* new_n=(struct node*)malloc(sizeof(struct node));
	new_n->left=NULL;
	new_n->right=NULL;
	new_n->data=data;
	return new_n;
}
int main() {
	struct node* root=newNode(5);
	return 0;
}

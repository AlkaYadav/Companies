// Given the sequence 3, 4, 33, 34, 43, 44, 333, 334, 343, ... and a number 'n',
//print the nth number in this sequence

#include <iostream>
using namespace std;

int sequence(int n){
	int a=3,b=4,c=0,d=0;
	int cnt=0;
	while(cnt<n){
		prev_a=a;
		prev_b=b;

		a=prev_a*10+3;
		cnt++;

		b=prev_a*10+4;
		cnt++;

		c=prev_b*10+3;
		cnt++;

		d=prev_b*10+4;
		cnt++;
	}
}
int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}

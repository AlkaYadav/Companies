#include <iostream>
using namespace std;

void distinctPalinSubstr(string str){
	int len=str.length();
	bool table[len][len];
	for(int i=0;i<len;i++){
		for(int j=0;j<len;j++){
			table[i][j]=false;
		}
	}
	for(int i=0;i<len;i++){
		table[i][i]=true;
	}
	for(int L=2;L<=len;L++){
		for(int i=0;i<=len-L;i++){
			int j=i+L-1;
			if(str[i]==str[j]){
				if(L==2){
					table[i][j]=true;
				}
				else{
					table[i][j]=table[i+1][j-1];
				}
			}
		}
	}
}



#include<algorithm>
#include<iostream>
using namespace std;

int compare(int a,int b){
	return a<b;
}
int main(){
	int a[]={1,5,2,7,3,8};
	sort(a,a+6,compare);
	for(int i=0;i<6;i++){
		cout<<a[i]<<" ";
	}
	return 0;
}

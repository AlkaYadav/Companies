#include <iostream>
#include<string>
#include<limits.h>
using namespace std;

void minimumPalnPartition(string str){
	int len=str.length();
	bool P[len][len];
	int C[len][len];

	for(int i=0;i<len;i++){
		P[i][i]=true;
		C[i][i]=0;
	}
}



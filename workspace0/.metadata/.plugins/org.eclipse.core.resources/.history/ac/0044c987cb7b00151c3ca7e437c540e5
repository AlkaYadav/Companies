#include <iostream>
#include<math.h>
#include<map>
using namespace std;

int factorial(int N){
	if(N<=0)
		return 1;
	return N*factorial(N-1);
}
int magicFraction(int N){
	map<int,int> m;
	int tmp=N;
	if(N==1)
		return 0;
	if(N==2)
		return 1;

	int fact=factorial(N);

	for(int i=2;i<=fact/2;i++){
		if(fact%i==0){
			cout<<"divided by "<<i<<endl;
			while(fact%i==0){
			     m[i]++;
			     fact=fact/i;
			}
		}
	}

	int cnt=1;
	map<int,int>::iterator it;
	cout<<"For "<<tmp<<endl;
	for(it=m.begin();it!=m.end();it++){
		cout<<it->first<<" "<<it->second<<endl;
		cnt++;
	}
	cout<<"For "<<tmp<<" cnt "<<cnt<<endl;
	if(cnt==2)
		return 1;
	return cnt;
}

int main(){
	int n;
	cin>>n;
	int cnt=0;
	cout<<magicFraction(n)<<endl;

	/*for(int i=1;i<=n;i++){
		cnt+=magicFraction(i);
	}*/
	cout<<"Total "<<cnt<<endl;
}

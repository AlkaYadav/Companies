#define ROW 100
#define COL 100

void DFSUtil(int a[ROW][COL],bool visited[ROW][COL],int i,int j,int rows,int cols){

}
void connectedIslands(int a[ROW][COL],int m,int n){
bool visited[ROW][COL];
for(int i=0;i<m;i++){
	for(int j=0;j<n;j++){
		visited[i][j]=false;
	}
}

for(int i=0;i<m;i++){
	for(int j=0;j<n;j++){
		if(visited[i][j]==false){
			DFSUtil(a,visited,i,j,m,n);
		}
	}
}
}

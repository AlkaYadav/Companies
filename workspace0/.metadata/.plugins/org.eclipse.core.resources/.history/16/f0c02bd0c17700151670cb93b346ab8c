#include<algorithm>
#include<iostream>
using namespace std;

int compare(int a,int b){
	return a<b;
}
int main(){
	int a[]={1,5,2,7,3,2};
	vector<a,a+6> v;
	sort(a,a+6,compare);
	make_heap(a,a+6);
	for(int i=0;i<6;i++){
		cout<<a[i]<<" ";

	}
	return 0;
}

//Find maximum value of Sum( i*arr[i]) with only rotations
//on given array allowed

void maxSum(int arr[],int len){
	int copyarr[2*len];
	for(int i=0;i<len;i++){
		copyarr[i]=arr[i];
		copyarr[i+len-1]=arr[i];
	}
}



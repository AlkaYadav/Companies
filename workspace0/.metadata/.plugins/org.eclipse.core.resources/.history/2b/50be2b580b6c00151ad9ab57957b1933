#include<iostream>
using namespace std;

void identityMatrix(int res[][26],int size){
	for(int i=0;i<size;i++){
		for(int j=0;j<size;j++){
			if(i==j){
				res[i][j]=1;
			}
			else{
				res[i][j]=0;
			}
		}
	}
}
void power(int mat[][26],int n){
	int res[26][26];
	identityMatrix(res,26);
	while(n-1){
		if(n%2==0){
			multiply(mat,mat);
			n=n/2;
		}
		else{
			multiply(res,mat);
			n--;
		}
	}
}

void printMatrix(int mat[][26],int size){
	for(int i=0;i<size;i++){
		for(int j=0;j<size;j++){
			cout<<mat[i][j]<<" ";
		}
		cout<<endl;
	}
}
int main(){
	int mat[2][2];
	for(int i=0;i<2;i++){
		for(int j=0;j<2;j++){
			cin>>mat[i][j];
		}
	}
	power(mat,2);
	printMatrix(mat,2);
}



#include <iostream>
#include<stack>
#include<limits.h>
using namespace std;

int minimumCoins(int coins[],int m,int V){
	int table[V+1][m+1];
	for(int i=0;i<=V;i++){
		table[i][0]=INT_MAX;
	}
	for(int i=0;i<=m;i++){
		table[0][i]=0;
	}
}


